{
  "name": "next-flask",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run next-dev\" \"npm run flask-dev\"",
    "next-dev": "next dev",
    "flask-dev": "python -m flask --app api/index run -p 5328",
    "build": "next build",
    "start": "next start",
    "install-deps": "python -m venv venv && . venv/bin/activate && pip install -r api/requirements.txt && npm install",
    "start-production": ". venv/bin/activate && concurrently \"python api/index.py\" \"next start\""
  },
  "dependencies": {
    "@types/node": "20.1.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "framer-motion": "^12.6.5",
    "next": "13.4.3",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  }
}